<!DOCTYPE html>
<html lang="ru">
  <head>
    <title> new_core index page </title>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="theme-color" content="#9CC2CE">
    
    <link href="assets/css/css/material.css"   rel="stylesheet">
    <link href="assets/css/css/style.css" rel="stylesheet">
    <link href="assets/css/css/froala.css" rel="stylesheet">
    <link href="assets/css/css/font.css"  rel="stylesheet">

    <script type="text/javascript">
        'use strict';
        var core = core || {};
        window.onload  = function(){
          core.global = {
            user : {
              id   : '<%= current_user._id %>',
              name : '<%= current_user.official_name %>',
            },
            provider: {
              id   : '<%= current_user.provider._id %>',
              name : '<%= current_user.provider.name %>',
            }
          }
          core.startAll();
        };
    </script>
  </head>
  
  <body>
    <script type="text/javascript" src="assets/build/application.min.js"></script>
    <script type="text/javascript" src="assets/js/shared/material.js"></script>
    <script type="text/javascript" src="assets/js/shared/jquery.js"></script>
    <script type="text/javascript" src="assets/js/shared/froala.js"></script>
  </body>
</html>
